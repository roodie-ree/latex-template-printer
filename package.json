{
  "name": "latex-template-printer",
  "version": "0.0.1",
  "description": "a template printing api using latex",
  "main": "app.js",
  "scripts": {
    "start": "nodemon -r ts-node/register src/app.ts | bunyan",
    "build": "tsc",
    "serve": "node dist/app.js | bunyan",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roodie-ree/latex-template-printer.git"
  },
  "keywords": [
    "latex",
    "template",
    "printer",
    "print",
    "printing"
  ],
  "author": "David Falk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roodie-ree/latex-template-printer/issues"
  },
  "homepage": "https://github.com/roodie-ree/latex-template-printer#readme",
  "dependencies": {
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "express": "4.16.4",
    "helmet": "3.12.1",
    "isolated": "1.0.0",
    "lodash": "4.17.11",
    "printer": "0.2.2",
    "rimraf": "2.6.2",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.5",
    "@types/chai": "4.1.7",
    "@types/express": "4.16.0",
    "@types/helmet": "0.0.42",
    "@types/lodash": "4.14.119",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.15",
    "@types/serve-favicon": "2.2.30",
    "chai": "4.2.0",
    "concurrently": "4.1.0",
    "mocha": "5.2.0",
    "nodemon": "1.17.5",
    "nyc": "13.1.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.2.2"
  }
}
